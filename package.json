{
  "name": "draught",
  "version": "3.0.0",
  "description": "Drawing tool in ES6 for creating diagrams using D3 with an extensible event driven architecture.",
  "main": "dist/draught.min.js",
  "scripts": {
    "test": "ava",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wildhoney/Draught.git"
  },
  "keywords": [
    "diagram",
    "drawing",
    "svg",
    "canvas",
    "draw"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "ignore": [
    "**/.*",
    "node_modules",
    "public",
    "tests"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/Draught/issues"
  },
  "homepage": "https://github.com/Wildhoney/Draught",
  "devDependencies": {
    "ava": "git+https://github.com/sindresorhus/ava.git",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^6.1.2",
    "bower": "^1.3.12",
    "express": "^4.13.4",
    "jsdom": "^8.4.0",
    "mousetrap": "^1.5.3",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "keo": "^5.0.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-standalone": "^0.2.0"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register",
      "./tests/helpers/setup-browser-env.js"
    ]
  }
}
